<!DOCTYPE html>
<html>
    <head>
        <title>Moltran - Responsive Admin Dashboard Template</title>
        <partial name="PartialViews/_MetaTags"></partial>
        <partial name="PartialViews/_Styles" ></partial>
    </head>

    <body class="fixed-left">
        
        <div id="wrapper">
        
            <partial name="PartialViews/_Header"></partial>

            <partial name="PartialViews/_AdminRightSideBar" ></partial>
            
            <div class="content-page">
                <!-- Start content -->
               @RenderBody()
               
               <partial name="PartialViews/_Footer"></partial>
            </div>
            
            <partial name="PartialViews/_AdminLeftSideBar" ></partial>
            
        </div>
        
        <partial name="PartialViews/_Scripts" ></partial>
        
    </body>
</html>